<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/textView17"
                android:layout_width="match_parent"
                android:layout_height="74dp"
                android:textSize="35dp"
                android:text="Registration Form" />
            <EditText
                android:id="@+id/team_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:hint="Enter Team Name"
                android:layout_margin="10dp"/>
            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Team Leader:"
                android:textSize="20dp"/>
            <EditText
                android:id="@+id/l_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="text"
                android:hint="Name"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/l_reg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="text"
                android:hint="Registration Number"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/l_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:hint="Email ID"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/l_ph_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:hint="Phone Number"
                android:layout_margin="10dp"/>
            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Member-1:"
                android:textSize="20dp"/>
            <EditText
                android:id="@+id/mem1_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="text"
                android:hint="Name"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/mem1_reg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="text"
                android:hint="Registration Number"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/mem1_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:hint="Email ID"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/mem1_ph_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:hint="Phone Number"
                android:layout_margin="10dp"/>
            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:text="Member-2:"
                android:layout_height="wrap_content"
                android:textSize="20dp"/>
            <EditText
                android:id="@+id/mem2_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="text"
                android:hint="Name"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/mem2_reg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="text"
                android:hint="Registration Number"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/mem2_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:hint="Email ID"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/mem2_ph_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:hint="Phone Number"
                android:layout_margin="10dp"/>
            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:text="Member-3:"
                android:layout_height="wrap_content"
                android:textSize="20dp"/>
            <EditText
                android:id="@+id/mem3_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="text"
                android:hint="Name"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/mem3_reg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="text"
                android:hint="Registration Number"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/mem3_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:hint="Email ID"
                android:layout_margin="10dp"/>
            <EditText
                android:id="@+id/mem3_ph_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:hint="Phone Number"
                android:layout_margin="10dp"/>

            <EditText
                android:id="@+id/password"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="Enter Password"
                android:layout_margin="10dp"
                android:inputType="textPassword" />

            <Button
                android:id="@+id/register_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="REGISTER"/>












        </LinearLayout>
    </ScrollView>
</LinearLayout>